{
  "name": "xililo/yii2-pdfjs",
  "description": "Previewer PDF File with PDF.js for Yii2 with CORS unrestricted",
  "authors": [
    {
      "name": "Sililo",
      "email": "sililoconnect@gmail.com"
    }
  ],
  "type": "library",
  "keywords": [
    "yii2",
    "widget",
    "pdfjs",
    "pdfviewer",
    "jquery"
  ],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/xililo/yii2-pdfjs/issues",
    "source": "https://github.com/xililo/yii2-pdfjs"
  },
  "require": {
    "php": ">=7.4.0",
    "yiisoft/yii2": "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.5.0"
  },
  "autoload": {
    "psr-4": {
      "xililo\\pdfjs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "xililo\\pdfjs\\tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
